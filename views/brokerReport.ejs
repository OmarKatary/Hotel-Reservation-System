<%- include partials/header %>

    <div class="container card">
        <h1 class="card-header" align='center'>Monthly Report</h1>
        <div class="card-text">
            <%=JSON.stringify(labels)%>
                <%=JSON.stringify(values)%>
        </div>
    </div>

    <div class="container card">
        <canvas id="myChart" data-labels="<%=JSON.stringify(labels)%>" data-values="<%=JSON.stringify(values)%>"></canvas>
    </div>





    <script>
        var ctx = document.getElementById('myChart').getContext('2d');
        var lab = $('#myChart').attr('data-labels');
        var val = $('#myChart').attr('data-values')
        // var lab = '<%=labels%>';
        // var val = '<%=values%>';
        var chart = new Chart(ctx, {
            type: 'bar',
            data: {
                labels: lab,
                datasets: [{
                    label: "Money From Hotels",
                    backgroundColor: 'rgb(252,168,60)',
                    borderColor: 'rgb(252,168,60)',
                    data: val
                }]
            }
        });

    </script>

